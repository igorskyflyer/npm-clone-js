{
	"name": "node-clone-js",
	"version": "1.1.0",
	"description": "🧬 A lightweight JavaScript utility allowing deep copy-by-value of nested objects, arrays and arrays of objects. 🪁",
	"engines": {
		"node": ">=12.17"
	},
	"sideEffects": false,
	"main": "./src/index",
	"exports": {
		".": {
			"require": "./src/index.js",
			"import": "./src/index.mjs"
		}
	},
	"module": "./src/index.mjs",
	"types": "./dist/index",
	"files": [
		"dist",
		"src"
	],
	"scripts": {
		"test": "mocha --slow 0",
		"pub": "npm test && tsc && npm publish"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/igorskyflyer/npm-clone-js"
	},
	"bugs": {
		"url": "https://github.com/igorskyflyer/npm-clone-js/issues"
	},
	"homepage": "https://github.com/igorskyflyer/npm-clone-js#readme",
	"keywords": [
		"clone",
		"copy",
		"array",
		"nested",
		"value",
		"utility",
		"lightweight",
		"objects",
		"igorskyflyer"
	],
	"author": "Igor Dimitrijević",
	"license": "MIT",
	"devDependencies": {
		"@types/chai": "^4.2.21",
		"@types/mocha": "^9.0.0",
		"chai": "^4.3.4",
		"mocha": "^9.0.3",
		"typescript": "^4.3.5"
	}
}
